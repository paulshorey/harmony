{
  "name": "@spiral/utils",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./index.tsx",
  "types": "./index.tsx",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,json}\"",
    "lint": "eslint . --fix --ext .tsx,.ts,.js --quiet",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:ci": "jest",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --fix --ext .tsx,.ts,.js --quiet",
      "prettier . -w"
    ]
  },
  "dependencies": {
    "next": "12.0.8",
    "nookies": "^2.5.2"
  },
  "devDependencies": {
    "@spiral/config": "*"
  }
}
