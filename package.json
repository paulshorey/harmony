{
  "name": "spiral-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "apis/*",
    "clis/*",
    "packages-browser/*",
    "packages-server/*",
    "packages-universal/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "turbo run format",
    "format:check": "prettier -c .",
    "lint": "yarn sort & yarn check-deps && turbo run lint",
    "lint-staged": "yarn sort & yarn check-deps && yarn workspaces run lint-staged",
    "sort": "sort-package-json \"apps/*/package.json\" \"packages/*/package.json\"",
    "storybook": "turbo run storybook",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "check-deps": "standard-monorepo circular-deps",
    "type-check": "turbo run type-check",
    "prepare": "husky install",
    "commit": "git add . && git-cz"
  },
  "resolutions": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.11"
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
